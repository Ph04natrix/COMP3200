{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main", "login"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging", "core:window:allow-close", "core:window:allow-minimize", "core:window:allow-toggle-maximize",
    "opener:default",
    "store:default",
    "http:default",
    "process:allow-exit"
  ]
}